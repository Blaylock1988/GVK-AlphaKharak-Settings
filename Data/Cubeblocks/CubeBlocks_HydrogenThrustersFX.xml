	<!-- LG -->
	<Definition xsi:type="MyObjectBuilder_ThrustDefinition">
		<!-- LargeBlockLargeHydrogenThrust -->
		<Id>
			<TypeId>Thrust</TypeId>
			<SubtypeId>LargeBlockLargeHydrogenThrust</SubtypeId>
		</Id>
		<!-- Normal Keen Properties 
    ...
    -->
		<ModExtensions>
			<ModComponents>
				<GameLogicComponent Name="MwiComponent"/>
			</ModComponents>
			<Group Name="ThrusterCompData">
				<Text Name="ID" Value="LargeBlockLargeHydrogenThrust"/>
				<!--REQUIRED! No specific value required Debugging  -->
				<Boolean Name="hideVanillaFlame" Value="true"/>
				<!-- Hide Vanilla Flame-->
				<Decimal Name="ThrustRateGain" Value="0.1"/>
				<Decimal Name="ThrustRateLoss" Value="0.4"/>
				<Boolean Name="UseEmissive" Value="false"/>
				<!-- Enable heatup/emissive material effect, off = more performant -->
				<Text Name="EmissiveMaterial" Value="heatup"/>
				<!-- custom material for emissive that react to thrust -->
				<Decimal Name="EmissiveRateGain" Value="0.005"/>
				<Decimal Name="EmissiveRateLoss" Value="0.01"/>
				<Text Name="EmitterSetup" Value="                    
                    Particle : MDB_ThrusterFlame_TFX;
                    Type: PARTICLE;
                    AtmoOnly: FALSE ;
                    ThrustMin : 0.05;
                    ThrustMax : 1;
                    Size:1;
                    Offset: 0;
                    VelocityScalingFrom: 0.25;
                    VelocityScalingTo: 1;
                    Dummies : thruster_flame;
                    =
                    Particle : MDB_ThrusterSmoke_TFX;
                    Type : PARTICLE;
                    AtmoOnly: FALSE ;
                    ThrustMin : 0.05;
                    ThrustMax : 1;
                    Size : 1;
                    Offset : 0;                    
                    VelocityScalingFrom: 0.25;
                    VelocityScalingTo: 1;
                    Dummies : thruster_flame;                                       
                    "/>
			</Group>
		</ModExtensions>
	</Definition>
