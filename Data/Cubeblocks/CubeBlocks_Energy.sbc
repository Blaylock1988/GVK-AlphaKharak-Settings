			<Components>
				<Component Subtype="SteelPlate" Count="2160"/><!-- 80 -->
				<Component Subtype="Construction" Count="1620"/><!-- 60 -->
				<Component Subtype="LargeTube" Count="324"/><!-- 12 -->
				<Component Subtype="SmallTube" Count="540"/><!-- 20 -->
				<Component Subtype="Motor" Count="324"/><!-- 12 -->
				<Component Subtype="Computer" Count="108"/><!-- 4 -->
				<Component Subtype="PowerCell" Count="27"/><!-- 1 -->
				<Component Subtype="Construction" Count="270"/><!-- 10 -->
				<Component Subtype="SteelPlate" Count="540"/><!-- 20 -->
			</Components>
			<CriticalComponent Subtype="PowerCell" Index="0"/>
