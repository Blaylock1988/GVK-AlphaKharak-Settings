				<Model BuildPercentUpperBound="0.33" File="Models\HugeHydrogenEngine3_Construction1.mwm"/>
				<Model BuildPercentUpperBound="0.66" File="Models\HugeHydrogenEngine3_Construction2.mwm"/>
				<Model BuildPercentUpperBound="1.00" File="Models\HugeHydrogenEngine3_Construction3.mwm"/>
			</BuildProgressModels>
			<BlockPairName>HugeHydrogenEngine3</BlockPairName>
			<MirroringY>Z</MirroringY>
